{
    "name": "serendipity_hq/commands-queues-bundle",
    "description": "Manage queues of commands and processes them in your Symfony app.",
    "type": "symfony-bundle",
    "keywords": [],
    "license": "MIT",
    "authors": [
        {
            "name": "Adamo Aerendir Crespi",
            "email": "hello@aerendir.me"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-json": "^1.5",
        "ext-pcntl": "*",
        "carbondate/carbon": "^1.33",
        "doctrine/common": "^2.5",
        "doctrine/doctrine-bundle": "^1.7",
        "doctrine/orm": "^2.6.3",
        "pagerfanta/pagerfanta": "^2.0",
        "sensio/framework-extra-bundle": "^5.1",
        "sensiolabs/ansi-to-html": "^1.1",
        "serendipity_hq/console-styles-bundle": "^1.1",
        "serendipity_hq/then-when": "^1.1",
        "symfony/console": "^4.2.3",
        "symfony/framework-bundle": "^4.0",
        "symfony/polyfill-php73": "^1.11",
        "symfony/process": "^4.0",
        "thecodingmachine/safe": "^0.1.15"
    },
    "require-dev": {
        "ext-ast": "*",
        "drenso/phan-extensions": "^2.4",
        "friendsofphp/php-cs-fixer": "^2",
        "phan/phan": "^1.3",
        "phpstan/phpstan": "@stable",
        "phpstan/phpstan-doctrine": "@stable",
        "phpstan/phpstan-phpunit": "@stable",
        "phpstan/phpstan-symfony": "@stable",
        "phpunit/phpunit": "^6||^7||^8",
        "rector/rector": "^0.5.0",
        "roave/security-advisories": "dev-master",
        "sensiolabs/security-checker": "^5",
        "sonata-project/admin-bundle": "^3.48",
        "symfony/var-dumper": "^4.0",
        "thecodingmachine/phpstan-safe-rule": "^0.1.3"
    },
    "suggest":{
        "sonata-project/admin-bundle": "Allows to view what's going on with your queues and commands jobs using automatic integration with SonataAdminBundle."
    },
    "autoload": {
        "psr-4": {
            "SerendipityHQ\\Bundle\\CommandsQueuesBundle\\": "src"
        },
        "exclude-from-classmap": ["/Tests/"]
    },
    "config": {
        "sort-packages": true
    }
}
