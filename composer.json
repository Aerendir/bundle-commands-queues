{
    "name": "serendipity_hq/commands-queues-bundle",
    "description": "Manage queues of commands and processes them in your Symfony app.",
    "type": "symfony-bundle",
    "keywords": [],
    "license": "MIT",
    "authors": [
        {
            "name": "Adamo Aerendir Crespi",
            "email": "hello@aerendir.me"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "ext-pcntl": "*",
        "carbondate/carbon": "^1.33",
        "doctrine/common": "^2.5",
        "doctrine/doctrine-bundle": "^1.7",
        "doctrine/orm": "^2.6.3",
        "pagerfanta/pagerfanta": "^2.0",
        "sensio/framework-extra-bundle": "^5.1",
        "sensiolabs/ansi-to-html": "^1.1",
        "serendipity_hq/console-styles-bundle": "dev-dev",
        "serendipity_hq/component-then-when": "dev-dev",
        "symfony/console": "^3.0|^4.0|^5.0",
        "symfony/framework-bundle": "^3.0|^4.0|^5.0",
        "symfony/polyfill-php73": "@stable",
        "symfony/process": "^3.0|^4.0|^5.0",
        "thecodingmachine/safe": "^1.0"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.4",
        "roave/security-advisories": "dev-master",
        "sonata-project/admin-bundle": "^3.48",
        "symfony/var-dumper": "^3.0|^4.0|^5.0"
    },
    "suggest":{
        "sonata-project/admin-bundle": "Allows to view what's going on with your queues and commands jobs using automatic integration with SonataAdminBundle."
    },
    "autoload": {
        "psr-4": {
            "SerendipityHQ\\Bundle\\CommandsQueuesBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SerendipityHQ\\Bundle\\CommandsQueuesBundle\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "bin": "echo 'bin not installed'",
        "post-install-cmd": ["@composer bin all install --ansi"],
        "post-update-cmd": ["@composer bin all update --ansi"]
    },
    "conflict": {
        "nikic/php-parser": "4.6.0"
    }
}
